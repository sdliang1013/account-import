<?xml version="1.0" encoding="UTF-8"?>
<excelImport>
    <repeatImport>
        <![CDATA[
			A2 = accountName,
			B2 = qq,
			C2 = mobile,
			D2 = handsel,
		]]>
        <endCode>End</endCode>
    </repeatImport>

    <!-- 校验器定义 -->
    <validators>
        <validator name="mobileValidator" value="com.caul.core.excelimport.validate.MobileValidator"/>
        <validator name="qqValidator" value="com.caul.core.excelimport.validate.QQValidator"/>
        <validator name="handselValidator" value="com.caul.core.excelimport.validate.HandselValidator"/>
    </validators>

    <!-- 需要校验的单元格 -->
    <cell-validators>
        <cell-validator cellname="B2">
            <validator name="qqValidator"/>
        </cell-validator>
        <cell-validator cellname="C2">
            <validator name="mobileValidator"/>
        </cell-validator>
        <cell-validator cellname="D2">
            <validator name="handselValidator"/>
        </cell-validator>
    </cell-validators>

</excelImport>